{% if IS_XHR %}
    {% extends "xhr.njk" %}
{% else %}
    {% extends "template.njk" %}
{% endif %}

{% set htmlClasses = "app-no-canvas-background" %}

{% block pageTitle %}
    {% if pageName %}
        {{pageName}}
    {% else %}
        Find Exporters
    {% endif %}
{% endblock %}

{% block head %}
    <!-- Google Tag Manager -->
    <script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
          new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
        j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
        'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
      })(window,document,'script','dataLayer','{{ googleTagManagerKey }}');</script>
    <!-- End Google Tag Manager -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!--[if !IE 8]><!-->
    <!-- build:css(src) /public/css/styles.min.css -->
        <base href="/">
        <link rel="stylesheet" href="css/styles.css">
    <!-- endbuild -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/prism/1.6.0/themes/prism.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/prism/1.6.0/prism.min.js"></script>
    <!--<![endif]-->
    <!--[if IE 8]>
    <link rel="stylesheet" href="/public/app-ie8.css">
    <![endif]-->
    <!--[if lt IE 9]>
    <script src="js/vendor/html5shiv.min.js"></script>
    <![endif]-->

    {% block styles %}{% endblock %}
{% endblock %}

{% block bodyStart %}
    <!-- Google Tag Manager (noscript) -->
    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id={{ googleTagManagerKey }}"
                      height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->
{% endblock %}

{% block bodyEnd %}
    <!-- build:js(src) /public/js/all.min.js -->
        <script src="/public/js/lib/govuk-design-system-all.js"></script>
        <script src="js/app.js"></script>
    <!-- endbuild -->

    <script>window.GOVUKFrontend.initAll()</script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/iframe-resizer/3.5.15/iframeResizer.min.js"></script>
    <!--[if lte IE 8]>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/iframe-resizer/3.5.15/ie8.polyfils.min.js"></script>
    <![endif]-->
    <script type="text/javascript">
      var domReady = function(a,b,c){b=document,c='addEventListener';b[c]?b[c]('DOMContentLoaded',a):window.attachEvent('onload',a)}
      domReady(iFrameResize({}, '.js-component-preview'))
    </script>
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
        m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', '{{ analyticsId }}', 'auto');
      ga('send', 'pageview');
    </script>
    {% block scripts %}{% endblock %}
{% endblock %}
