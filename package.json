{
  "name": "prospects-fe",
  "version": "1.0.0",
  "description": "Data Hub Find Exporters",
  "main": "server.js",
  "engines": {
    "node": "8.11.1",
    "npm": "5.6.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "scripts": {
    "clean": "del src/app/public/dist",
    "preversion": "npm run lint && npm test",
    "postversion": "git push && git push --tags",
    "tag-master": "git tag v${npm_package_version}-master && git push --tags",
    "test": "NODE_ENV=development; SSO_BYPASS=true; jest",
    "test:unit": "jest",
    "start": "NODE_ENV=production node --use-strict server.js",
    "watch": "export LOG_LEVEL=debug; nodemon src/app/cluster.js --ignore 'src/public/**/*' --ignore 'src/test/**/*'",
    "grunt": "grunt dist",
    "build": "npm run clean && npm run prod",
    "dist": "npm run build:dist && npm run start",
    "heroku-postbuild": "npm run build",
    "lint": "npm-run-all lint:all:*",
    "lint:all:js": "eslint ./src ./config",
    "lint:all:styles": "sass-lint -v -q -c .sass-lint.yml src/public/scss/**/*.scss",
    "dev": "webpack --mode development",
    "prod": "webpack --mode production",
    "develop": "run-p watch:js:*",
    "watch:js:client": "webpack --watch --progress --mode development",
    "watch:js:server": "export LOG_LEVEL=debug; nodemon src/app/cluster.js --ignore 'src/public/**/*' --ignore 'src/test/**/*'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uktrade/data-science-frontend.git"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": "eslint",
    "*.scss": "sass-lint -v -q -c .sass-lint.yml"
  },
  "contributors": [
    "John Fieldman <hello@johnfieldman.com> (https://github.com/yozzo)",
    "Ryan Holmes (https://github.com/RyanHolmesDIT)",
    "Felippo Raimondi <filipporai@gmail.com> (https://github.com/PippoRaimondi)",
    "Graham Veal"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uktrade/data-science-frontend/issues"
  },
  "homepage": "https://github.com/uktrade/data-science-frontend#readme",
  "devDependencies": {
    "browser-sync-webpack-plugin": "^2.2.2",
    "dotenv": "^6.0.0",
    "eslint": "^5.6.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "grunt": "^1.0.3",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "1.1.0",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-cssmin": "2.2.1",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "3.3.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "2.3.1",
    "grunt-mkdir": "1.0.0",
    "grunt-usemin": "3.1.1",
    "jasmine": "3.0.0",
    "jasmine-core": "2.99.1",
    "jest": "^23.6.0",
    "jest-codemods": "^0.16.0",
    "load-grunt-tasks": "3.5.2",
    "mocha": "^5.2.0",
    "mocha-circleci-reporter": "0.0.3",
    "mocha-multi": "^1.0.1",
    "mocha-webpack": "^1.1.0",
    "mockdate": "2.0.2",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "proxyquire": "1.8.0",
    "sass-lint": "^1.12.1",
    "supertest": "^3.3.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "array-from": "^2.1.1",
    "autoprefixer": "^9.3.1",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "compression": "1.7.2",
    "connect-redis": "3.3.3",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "date-fns": "^1.29.0",
    "dateformat": "3.0.3",
    "del-cli": "^1.1.0",
    "element-closest": "^2.0.2",
    "es6-promise": "^4.2.5",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fast-sass-loader": "^1.4.6",
    "get-form-data": "^2.0.0",
    "govuk-frontend": "^1.3.0",
    "grunt-sass": "^3.0.1",
    "hawk": "7.0.7",
    "history": "^4.7.2",
    "html5shiv": "^3.7.3",
    "image-webpack-loader": "^4.6.0",
    "morgan": "^1.9.1",
    "nunjucks": "^3.1.3",
    "postcss-loader": "^3.0.0",
    "qs": "^6.5.2",
    "raven": "^2.6.4",
    "redis": "2.8.0",
    "request": "^2.88.0",
    "resolve-url-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "uuid": "^3.3.2",
    "webpack": "^4.23.1",
    "webpack-assets-manifest": "^3.1.0",
    "webpack-merge": "^4.1.4",
    "winston": "2.4.2"
  },
  "sasslintConfig": ".sass-lint.yml"
}
